#MY-all{
  width:100%;
  height:30px;
  color:black;
  font-size: 16px;
  text-align: center;
}
#MY{
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:space-between;

}
#MY-img{
  width: 150rpx;
  height: 150rpx;
  margin: 20px 0 10px 0;
}
#MY-img image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
#MY-p{ 
  width: 60px;
  height:25px;
  display: flex;
  align-self: center;
}
#MY-p p{
  font-size: 16px;
  color: #000;
}
#top{
  width:100%;
  height:70px;
  margin:20px 0 25px 0;
}
#top view{
  height:35px;
  margin:0 10px;
  background: ghostwhite; 
  display: flex;
  justify-content: space-between;
}
 #top view text{
   color:black;
   font-size: 14px;
 }
#address{
  margin-bottom: 10px;
}
#address .nav{
  font-size: 14px;
}
